pub mod example_provider;

use async_trait::async_trait;

#[async_trait]
pub trait PayoutConnector: Send + Sync {
    async fn create_payout(
        &self,
        idempotency_key: &str,
        amount: i64,
        currency: &str,
        destination_iban: &str,
        recipient_name: &str,
        reference: Option<&str>,
    ) -> Result<Option<String>, anyhow::Error>;
}
